# Cypress Generator Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API Configuration (Required for AI features)
OPENAI_API_KEY=your_openai_api_key_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True

# Server Configuration
PORT=5000
HOST=0.0.0.0

# Optional: Custom OpenAI Model
# OPENAI_MODEL=gpt-3.5-turbo

# Optional: Custom Timeouts (in milliseconds)
# PAGE_LOAD_TIMEOUT=30000
# DEFAULT_COMMAND_TIMEOUT=10000

# Optional: Custom User Agent
# USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Optional: Enable/Disable Features
# ENABLE_AI_SUGGESTIONS=true
# ENABLE_LIVEWIRE_DETECTION=true
# ENABLE_PAGE_OBJECT_GENERATION=true
